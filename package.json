{
  "name": "@processmaker/vue-form-elements",
  "version": "0.14.4",
  "description": "Reusable VueJS Based Form Elements styled with Bootstrap 4",
  "scripts": {
    "serve": "NODE_ENV=standalone vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-bundle": "vue-cli-service build --target lib --name vue-form-elements ./src/components/index.js",
    "build-watch": "vue-cli-service build --target lib --name vue-form-elements --watch ./src/components/index.js"
  },
  "main": "./dist/vue-form-elements.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@tinymce/tinymce-vue": "^2.0.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.7",
    "jquery": "^3.3.1",
    "lodash-es": "^4.17.21",
    "luxon": "^1.11.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "popper.js": "^1.14.4",
    "tinymce": "^5.0.1",
    "v-calendar": "^0.9.7",
    "validatorjs": "^3.14.2",
    "vue": "^2.5.16",
    "vue-bootstrap-datetimepicker": "^5.0.1",
    "vue-uniq-ids": "^1.0.0",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@panter/vue-i18next": "^0.15.0",
    "@types/jest": "^24.0.18",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "i18next": "^15.1.1",
    "mustache": "^3.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-multiselect": "^2.1.6",
    "vue-template-compiler": "^2.5.16"
  },
  "peerDependencies": {
    "mustache": "^3.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/processmaker/vue-form-elements/issues"
  },
  "homepage": "https://www.github.com/processmaker/vue-form-elements",
  "keywords": [
    "html",
    "vuejs",
    "form"
  ],
  "license": "MIT"
}
