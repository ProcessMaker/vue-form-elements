{
  "name": "@processmaker/vue-form-elements",
  "version": "0.14.4",
  "description": "Reusable VueJS Based Form Elements styled with Bootstrap 4",
  "scripts": {
    "serve": "NODE_ENV=standalone vue-cli-service serve dev/main.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/vue-form-elements.ssr.js",
  "browser": "./dist/vue-form-elements.min.js",
  "module": "./dist/vue-form-elements.esm.js",
  "files": [
    "dist/*",
    "src/**"
  ],
  "dependencies": {
    "@tinymce/tinymce-vue": "^2.0.0",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.7",
    "jquery": "^3.3.1",
    "lodash-es": "^4.17.21",
    "luxon": "^1.11.3",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "popper.js": "^1.14.4",
    "tinymce": "^5.0.1",
    "v-calendar": "^0.9.7",
    "validatorjs": "^3.14.2",
    "vue": "^2.5.16",
    "vue-bootstrap-datetimepicker": "^5.0.1",
    "vue-uniq-ids": "^1.0.0",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@panter/vue-i18next": "^0.15.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^24.0.18",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.23",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "i18next": "^15.1.1",
    "minimist": "^1.2.5",
    "mustache": "^3.0.1",
    "postcss": "^8.4.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.2",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.44.0",
    "sass-loader": "^7.0.3",
    "vue-multiselect": "^2.1.6",
    "vue-template-compiler": "^2.5.16"
  },
  "peerDependencies": {
    "mustache": "^3.0.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "bugs": {
